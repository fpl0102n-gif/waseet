# Example environment variables for local development
# Copy this file to .env and replace placeholder values.

# --- Supabase ---
# Project URL and anon public key (never use service role key client-side)
VITE_SUPABASE_URL="https://YOUR_PROJECT_ID.supabase.co"
VITE_SUPABASE_PROJECT_ID="YOUR_PROJECT_ID"
VITE_SUPABASE_PUBLISHABLE_KEY="YOUR_ANON_PUBLIC_KEY"

# --- Telegram Bot (if features enabled) ---
VITE_TELEGRAM_BOT_USERNAME="your_bot_username"
VITE_TELEGRAM_SUPPORT_HANDLE="@support_handle"

# --- Exchange Feature (optional defaults) ---
VITE_EXCHANGE_DEFAULT_FROM="USD"
VITE_EXCHANGE_DEFAULT_TO="DZD"

# --- UI / Misc ---
VITE_APP_ENV="development"
VITE_LOG_LEVEL="info"

# Proxy bypass (set in shell, shown here for reference only)
# NO_PROXY="localhost,127.0.0.1,*.supabase.co"

# Never commit real secrets. Keep .env out of version control.
